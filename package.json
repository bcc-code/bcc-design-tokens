    "preview": "vite preview"
  },
  "peerDependencies": {
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "token-transformer": "^0.0.30",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
